{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "WhatisADF"
		},
		"ls_bronze_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_bronze'"
		},
		"ls_landing_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_landing'"
		},
		"ls_bronze_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfsourcestore.dfs.core.windows.net/"
		},
		"ls_landing_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfsourcestore.dfs.core.windows.net/"
		},
		"tr_main_pipeline_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/56d52e0f-239f-4ef6-a999-8297559b8172/resourceGroups/Databricks-rg/providers/Microsoft.Storage/storageAccounts/adfsourcestore"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "WhatisADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_bronze_accountKey": {
						"value": "[parameters('ls_bronze_accountKey')]"
					},
					"ls_landing_accountKey": {
						"value": "[parameters('ls_landing_accountKey')]"
					},
					"ls_bronze_properties_typeProperties_url": {
						"value": "[parameters('ls_bronze_properties_typeProperties_url')]"
					},
					"ls_landing_properties_typeProperties_url": {
						"value": "[parameters('ls_landing_properties_typeProperties_url')]"
					},
					"tr_main_pipeline_properties_typeProperties_scope": {
						"value": "[parameters('tr_main_pipeline_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}